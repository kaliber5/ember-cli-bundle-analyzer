{
  "name": "ember-cli-bundle-analyzer",
  "version": "0.2.1",
  "description": "Analyze the size and contents of your Ember app's bundles",
  "keywords": [
    "ember-addon",
    "bundlesize",
    "bundle-analyzer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliber5/ember-cli-bundle-analyzer"
  },
  "license": "MIT",
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "mocha node-tests/**/*-test.js --recursive"
  },
  "dependencies": {
    "broccoli-concat-analyser": "^4.3.4",
    "debug": "^4.1.1",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-version-checker": "^5.1.1",
    "hash-files": "^1.1.1",
    "intercept-stdout": "^0.1.2",
    "node-html-light": "^1.3.0",
    "sane": "^4.1.0",
    "tmp": "^0.2.1",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "chai-http": "^4.3.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.17.0",
    "ember-cli-addon-tests": "^0.11.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.17.3",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.9.1",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "history-support-middleware",
    "demoURL": "https://cdn.rawgit.com/kaliber5/ember-cli-bundle-analyzer/bceb55a7/docs/demo.html"
  }
}
